to-report get_uavs_random[quant_uavs]
  let list_config_uavs []
  
  repeat quant_uavs [ 
    let new_conf []
    ;; item 0 - recurso
    ;; item 1 - sensores
    ;; item 2 - posição x
    ;; item 3 - posição y

    set new_conf lput 0 new_conf  ;; Eles iniciam com Zero pois é carregado depois a quantidade de energia

    let list_sensores []
    ;let quant_sensores one-of [1 2 3] ;; quantidade de sensores que o vant pode ter
    let quant_sensores one-of [1 2] ;; quantidade de sensores que o vant pode ter
    print quant_sensores

    let sensores_disponiveis [0 1 2 3]
    repeat quant_sensores [
      let ss one-of sensores_disponiveis
      set list_sensores lput ss list_sensores ;; add na lista de sensores do vants
      set sensores_disponiveis remove ss sensores_disponiveis ;; tira da lista de sensores disponiveis para não repetir no mesmo vant
    ]
    set new_conf lput list_sensores new_conf
    
    set new_conf lput round random-xcor new_conf
    set new_conf lput round random-ycor new_conf

    ;set new_conf lput random (length item 0 quality-sensors) new_conf_task ;; sao 4 tipos de alvos

    set list_config_uavs lput new_conf list_config_uavs
  ]  
  
    print (word "  ;; size cenário: "max-pxcor "x" max-pycor)
    print (word "to-report get_" quant_uavs  "_uavs_" max-pxcor "x" max-pycor)
    print "  report "
    print_random list_config_uavs
    print "end"


  
  report list_config_uavs
end

