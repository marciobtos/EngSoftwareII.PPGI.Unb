to print_random[list_rand]
  print "     ["
  foreach list_rand [print word "       " ?]
  print "     ]"
end

to-report get_tasks_random [quant_tarefas]
  let list_config_task []
  
  repeat quant_tarefas [ 
    let new_conf_task []
    ;[a     x  y rc   s  t alpha] 
    ;[tipo_alvo x y recurso_consumido, estimulo, timeout, alpha]
    set new_conf_task lput random (length item 0 quality-sensors) new_conf_task ;; sao 4 tipos de alvos
    set new_conf_task lput round random-xcor new_conf_task
    set new_conf_task lput round random-ycor new_conf_task
    set new_conf_task lput 10 new_conf_task  ;; Estou fazendo todas as tarefas com o mesmo tamanho
    set new_conf_task lput 0.6 new_conf_task ;; aqui é padrão, mas depois é atribuido o valor deseado em cada execução (no setup)
    set new_conf_task lput 10 new_conf_task  ;; Não estou usando timeout no resto da aplicacao mas vou deixar
    set new_conf_task lput 0.6 new_conf_task ;; alpha é o peso para a distância..

    set list_config_task lput new_conf_task list_config_task
  ]  
  
  
  print (word "  ;; size cenário: "max-pxcor "x" max-pycor)
  print word "to-report get_tasks_" quant_tarefas 
  print "  report "
  print_random list_config_task
  print "end"
  
  report list_config_task
end

